<script type="text/javascript">
  
  function leapYear(){
	  
  var output =" " ;
  var year = document.getElementById("year").value;
  //var output = String(document.leap.result.value);
  output = (year%4==0)?"leap year":"not leap year";
  document.getElementById("leap").innerHTML="It is "+output;
  }
